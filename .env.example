# Frontend Stripe publishable key (starts with pk_test_ for testing, pk_live_ for production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Backend Stripe secret key (starts with sk_test_ for testing, sk_live_ for production)
STRIPE_SECRET_KEY=sk_test_your_key_here

# Stripe Webhook Secret (starts with whsec_)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Optional: Server port (defaults to 5000 if not set)
PORT=5000
